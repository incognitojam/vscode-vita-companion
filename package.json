{
  "name": "vscode-vita-companion",
  "displayName": "vita-companion",
  "description": "VS Code extension for vitacompanion",
  "version": "1.0.0",
  "publisher": "Cameron Clough",
  "author": "Cameron Clough (https://github.com/incognitojam)",
  "repository": "https://github.com/incognitojam/vscode-vita-companion",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vita",
    "companion"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Vita: Hello World"
      },
      {
        "command": "extension.connect",
        "title": "Vita: Connect",
        "icon": "debug-disconnect"
      },
      {
        "command": "extension.destroy",
        "title": "Vita: Kill all running applications",
        "icon": "trash"
      },
      {
        "command": "extension.launch",
        "title": "Vita: Launch application by Title ID",
        "icon": "play"
      },
      {
        "command": "extension.reboot",
        "title": "Vita: Reboot console",
        "icon": "debug-restart"
      },
      {
        "command": "extension.screenOn",
        "title": "Vita: Turn screen on"
      },
      {
        "command": "extension.screenOff",
        "title": "Vita: Turn screen off"
      },
      {
        "command": "extension.vpk",
        "title": "Vita: Install VPK from path"
      },
      {
        "command": "extension.upload",
        "title": "Vita: Upload file over FTP",
        "icon": "arrow-up"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "eslint": "^9.12.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  }
}
